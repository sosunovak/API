									"});",
									"pm.test(\"Varify that value are correct\", function () {",
									"    let responseJson = pm.response.json();",
									"    responseJson.forEach((product) => {",
									"        pm.expect(product.product_id).to.be.a('number');",
									"        pm.expect(product.name).to.be.a('string')",
									"    });",
									"});",
									"pm.test(\"Response time is less than 200ms\", function () {",
									"\tpm.expect(pm.response.responseTime).to.be.below(200);",
